{
  "name": "cosc-3380-project",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "build": "node scripts/build.js",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install --legacy-peer-deps && cd ../server && npm install",
    "clean": "rm -rf client/dist server/dist client/node_modules server/node_modules node_modules",
    "clean-all": "rm -rf client/dist server/dist",
    "typecheck:server": "cd server && npx tsc --noEmit",
    "typecheck:client": "cd client && npx tsc --noEmit",
    "typecheck": "npm run typecheck:server && npm run typecheck:client"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "pg": "^8.16.3"
  }
}
